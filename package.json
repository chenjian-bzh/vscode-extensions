{
  "name": "vconsole",
  "displayName": "vconsole-bootstrap",
  "description": "vconsole bootstrap tool, you can start or debug web and node process using one command",
  "version": "0.0.2",
  "publisher": "chenjian-bzh",
  "icon": "images/vconsole.png",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.vconsole.dev",
    "onCommand:extension.vconsole.debug",
    "onCommand:extension.vconsole.createDebugConfiguration"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.vconsole.dev",
        "title": "dev",
        "category": "vconsole"
      },
      {
        "command": "extension.vconsole.debug",
        "title": "debug",
        "category": "vconsole"
      },
      {
        "command": "extension.vconsole.createDebugConfiguration",
        "title": "createDebugConfiguration",
        "category": "vconsole"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
